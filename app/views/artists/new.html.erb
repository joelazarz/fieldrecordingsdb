<div class="f-container-row">

<div class="f-head">New Artist</div>

<div class="f-form">

<%= form_for @artist do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br><br>
<%= f.label :description %><br>
<%= f.text_area :description %><br><br>
<%= f.label "Photo" %><br>
<%= f.file_field :avatar %><br><br>
<%= f.submit "Submit" %><br>
<% end %>

</div>

</div>