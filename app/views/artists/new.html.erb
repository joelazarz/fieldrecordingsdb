<div class="container">

<div class="artist-header">
New Artist
</div>

<div class="artist-form">

<%= form_for @artist do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br><br>
<%= f.label :description %><br>
<%= f.text_area :description %><br><br>
<%= f.label "Photo" %><br>
<%= f.file_field :avatar %><br><br>
<%= f.submit "Submit" %><br>
<% end %>

</div>

</div>