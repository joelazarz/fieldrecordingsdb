<h1>New Artist</h1>

<div class="form">

<%= form_for @artist do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :description %>
<%= f.text_area :description %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label "Photo" %>
<%= f.file_field :avatar %>
<%= f.submit "Submit" %>
<% end %>

</div>
