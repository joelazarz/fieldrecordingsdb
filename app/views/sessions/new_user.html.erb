<div class="container">


<div class="user-header">
<h1>User Log In</h1>
</div>

<div class="user-form">

<%= form_tag "/sessions/user" do %>
<%= label_tag "Username" %><br>
<%= text_field_tag :username %><br>
<%= label_tag "Password" %><br>
<%= password_field_tag :password %><br>
<%= submit_tag "Log in" %>
<% end %>

</div>

</div>