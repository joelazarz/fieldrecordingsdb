<div class="container">


<div class="user-header">
<h1>New User</h1>
</div>

<div class="user-form">

<%= form_for @user do |f| %>
<%= f.label :email %><br>
<%= f.text_field :email %><br>
<%= f.label :username %><br>
<%= f.text_field :username %><br>
<%= f.label :password %><br>
<%= f.password_field :password %><br>
<%= f.submit "Submit" %><br>
<% end %>

</div>

</div>